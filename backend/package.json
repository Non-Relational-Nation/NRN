    "mongoose": "^8.17.0",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
