{
  "name": "nrn-backend",
  "version": "1.0.0",
  "description": "Non-Relational Nation - Social Media Backend",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsx build src/server.ts --out-dir=dist",
    "build:tsc": "tsc --project tsconfig.prod.json",
    "start": "node dist/server.js",
    "dev": "dotenvx run -- tsx watch ./src/server.ts",
    "prod": "dotenvx run -- node --import tsx ./src/server.ts"
  },
  "keywords": [
    "social-media",
    "non-relational",
    "typescript",
    "backend"
  ],
  "author": "Non-Relational Nation",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@dotenvx/dotenvx": "^1.48.4",
    "@fedify/express": "^0.2.1",
    "@fedify/fedify": "^1.7.7",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "jose": "^6.0.12",
    "mongoose": "^8.17.0",
    "multer": "^2.0.2",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
